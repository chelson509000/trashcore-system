{
  "name": "Trash-Core",
  "description": "A WhatsApp bot based on BaileysJS (TypeScript + JavaScript) with Multi-Device support.",
  "repository": "https://github.com/anonphoenix007/CH13",
  "logo": "https://telegra.ph/file/7aa528767ed96ddc17bc2.jpg",
  "keywords": ["nodejs", "bot", "whatsapp", "automation", "baileys", "multi-device"],
  "env": {
    "OWNER_NAME": {
      "description": "Enter your owner name here",
      "value": "Trashcore"
    },
    "BOT_PREFIX": {
      "description": "Bot command prefix",
      "value": "."
    },
    "ANTIDELETE": {
      "description": "Set true to detect deleted messages",
      "value": "true"
    },
    "AUTO_TYPE": {
      "description": "Set true for fake typing status",
      "value": "true"
    },
    "AUTO_RECORD": {
      "description": "Set true for fake recording status",
      "value": "false"
    },
    "AUTO_STATUS": {
      "description": "Set true for bot to view status",
      "value": "true"
    },
    "PACK_NAME": {
      "description": "Sticker pack name",
      "value": "Trashcore pack"
    },
    "AUTHOR": {
      "description": "Sticker author name",
      "value": "Trashcore"
    },
    "SESSION_ID": {
      "description": "Your WhatsApp session ID (required)",
      "required": true
    },
    "APP_NAME": {
      "description": "Heroku app name (used by keepalive.js)",
      "required": true
    }
  },
  "buildpacks": [
    { "url": "heroku/nodejs" },
    { "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest" },
    { "url": "https://github.com/clhuang/heroku-buildpack-webp-binaries.git" }
  ],
  "formation": {
    "worker": { "quantity": 1, "size": "eco" },
    "web": { "quantity": 1, "size": "eco" }
  }
}
